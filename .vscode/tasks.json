{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/csv-compare-csharp.sln"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "rebuild",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/csv-compare-csharp.sln",
        "--no-incremental",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary",
        "/p:Configuration=Debug",
        "/p:Platform=Any CPU"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}


  
  